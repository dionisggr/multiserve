{
  "name": "backend-template",
  "description": "",
  "version": "1.0.0",
  "main": "server.js",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "redis": "redis-server",
    "redis:close": "redis-cli shutdown",
    "migrations": "knex --knexfile src/db/knexfile.js migrate:latest",
    "migrations:create": "knex --knexfile src/db/knexfile.js migrate:make",
    "migrations:rollback": "knex --knexfile src/db/knexfile.js migrate:rollback --all",
    "migrations:reset": "npm run migrations:rollback && npm run migrations",
    "rollback": "npm run migrations:rollback",
    "reset": "npm run migrations:reset && npm run seeds",
    "seeds": "knex --knexfile src/db/knexfile.js seed:run && mv ",
    "seeds:create": "knex --knexfile src/db/knexfile.js seed:make",
    "login": "npx supabase login",
    "db:pull": "npx supabase db remote commit",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "db:backup": "src/db/backup/index.sh",
    "setup": "npm run db:pull && npm run db:start && npm run reset && source src/aliases.sh",
    "setup:aliases": "source src/aliases.sh"
  },
  "dependencies": {
    "backblaze-b2": "^1.7.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "connect-redis": "^7.0.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-xss-sanitizer": "^1.1.6",
    "fs": "^0.0.1-security",
    "helmet": "^6.1.5",
    "joi": "^17.9.1",
    "js-yaml": "^4.1.0",
    "knex": "^2.4.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "node-fetch": "^2.6.9",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "pino-pretty": "^10.0.0",
    "redis": "^4.6.6",
    "supabase": "^1.50.14",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@sendgrid/mail": "^7.7.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.22",
    "supabase-cli": "^0.0.21"
  }
